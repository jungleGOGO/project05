<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tsherpa.team35.per.QnaMapper">

    <select id="getList" resultType="com.tsherpa.team35.entity.Qna">
        select qno, title, content, author, resdate, lev, par
        FROM qna order BY par DESC,lev ASC, qno ASC, qno desc limit #{postStart}, #{postCount}
    </select>

    <select id="getCount" resultType="integer">
        select COUNT(*) from qna
    </select>
 </mapper>